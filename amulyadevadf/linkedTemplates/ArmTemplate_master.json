{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "amulyadevadf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabaseDevLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseDevLS'"
		},
		"filetofile_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'filetofile'"
		},
		"AzureSqlDatabaseDevLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "usracipd194.database.windows.net"
		},
		"AzureSqlDatabaseDevLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ExtractDB"
		},
		"AzureSqlDatabaseDevLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Amulya"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "amulyadevadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabaseDevLS_password": {
						"value": "[parameters('AzureSqlDatabaseDevLS_password')]"
					},
					"filetofile_connectionString": {
						"value": "[parameters('filetofile_connectionString')]"
					},
					"AzureSqlDatabaseDevLS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseDevLS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseDevLS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseDevLS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseDevLS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseDevLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}